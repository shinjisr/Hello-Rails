   <!-- Omit this iteration by using collection params of render in index.html.erb -->
    <tr>
      <td><%= link_to "投給他", vote_candidate_path(candidate), method: "post", data: { confirm: "確定投給 #{candidate.name}？"}, class:"btn btn-danger btn-xs" %></td> <!--btn = button, btn-danger = red color, btn-xs = small size. -->
      <td><%= candidate.name %>(年齡：<%= candidate.age %> 歲)</td>
      <td><%= user_gender(candidate.gender) %><%#Calling user_gender from app/helpers in every loaded file. %></td>
      <td><%= candidate.party %></td>
      <td><%= candidate.politics %></td>
      <td><%= candidate.vote_logs.size %></td>

      <td>
        <%= link_to "編輯", edit_candidate_path(candidate) %>
        <%= link_to "刪除", candidate_path(candidate), method: "delete", data: { confirm: "確認刪除" } %>
      </td>
    </tr>